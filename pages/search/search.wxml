<view class="containers" >
<!-- 搜索栏 -->
<view style="display:flex; justify-content: center; margin-top: 10px;">
  <view class="navv">
      <view class="search">
        <image bind:tap="searchs" src="/img/搜索.png" mode="" />
      </view>
      <view class="fancy-bg"></view>
      <input type="text" bindblur="inputed" class="input" placeholder="{{contents}}" id="search" />
    </view>
</view>
<view class="syl">搜索历史</view>
<view class="kuai">
  <view class="items" wx:for="{{items}}"><text>{{item}}</text></view>
</view>
</view>