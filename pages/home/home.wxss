.zhezhao{
  height: 31px;
  padding-left: 8vw;
  background-color: rgb(255, 255, 255,1);
  width: 100vw;
  position: absolute;
  z-index: 2000;
  
  }
  .zhezhao::after {
     content: ""; /* 必须定义content */
     position: absolute;
      bottom: 0; /* 紧贴底部 */
      left: 5vw; /* 从左边开始 */
      width: 90vw; /* 占据一半的宽度 */
      height: 0.5px; /* 边框厚度 */
      background-color: #e0e0e0; /* 自定义颜色 */
    }
