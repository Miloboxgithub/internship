.custom-nav-bar {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  height: 11vh;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background: linear-gradient(to bottom, rgba(80, 177, 178, 1), rgba(124, 222, 210, 1));
  /* 渐变背景 */
  padding: 0 16px;
  z-index: 1000;
  /* 确保导航栏在最顶层 */
  transition: top 0.3s ease-in-out;
  /* 平滑过渡效果 */
  display: flex;
  justify-content: center;
}

