  color: rgba(166, 166, 166, 1);
  text-align: center;
  vertical-align: top;
  display: flex;
  align-items: center;
}

.queren {
  margin-right: 3vw;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 56px;
  height: 30px;
