<scroll-view class="containers" scroll-y="true">
  <custom-loading show="{{lolo}}" title="加载中..."></custom-loading>
  <!-- 搜索选择栏 -->
  <view class="nav">
    <view class="nav1" id="types1" bindtap="changetypes" style="color: {{lei=='类型'?'rgba(25, 25, 25, 1)':'rgba(80, 177, 178, 1)'}};"><text>{{lei}}</text>
      <image src="{{types1?'/img/shang.png':'/img/xia.png'}}" mode="" />
    </view>
    <view class="nav3" id="types2" bindtap="changetypes" style="color: {{zhuang=='状态'?'rgba(25, 25, 25, 1)':'rgba(80, 177, 178, 1)'}};"><text>{{zhuang}}</text>
      <image src="{{types2?'/img/shang.png':zhuang=='状态'?'/img/xia.png':'/img/xiab.png'}}" mode="" />
    </view>
    <view class="navv">
      <view class="search" bind:tap="searchs">
        <image src="/img/搜索.png" mode="" />
      </view>
      <view class="fancy-bg"></view>
      <input type="text" bindconfirm="inputed" model:value="{{inputValue}}"  class="input" placeholder="请输入搜索内容"  id="search" />
    </view>
  </view>
  <view class="items">
    <view class="item" wx:for="{{coitem}}" wx:key="index"  data-id="{{index}}">
 <view class="boox">
  <view class="abc" bind:tap="dianji" data-id="{{index}}"><image src="/pkgA/img/圆形 1.png" class="blacks" mode=""/><image src="/pkgA/img/圆形 2.png"  style="display: {{item.isdian?'':'none'}};"  class="blues" mode=""/></view>
      <view class="coicon">
        <image src="{{item.icon}}" mode="aspectFit" lazy-load="false" mode="" />
      </view>
 </view>
      <view class="info" bind:tap="navigates" data-url="/pkgA/pages/detail/detail"  data-id="{{index}}">
        <text class="cotitle" selectable="false" space="false" decode="false">{{item.jobPosition}} | {{item.name}}</text>
        <view class="tags">
          <view class="tt">
            <view class="tag" wx:for="{{item.tags}}" wx:for-item="tag" wx:key="ttt">{{tag.title}}</view>
          </view>
          <view class="iszhao {{item.iszhao?'gg':'rr'}}">{{item.iszhao?'招募中':'已结束'}}</view>
        </view>
        <view class="fuji">
          <view class="times">截止时间：{{item.time}}</view>
          <view class="sums">
            <image src="/img/浏览.png" mode="" />
            <text>{{item.sum}}</text>
          </view>
        </view>
      </view>
    </view>
  </view>
  <view class="chico" wx:if="{{numbs!=0}}">
  <view style="width: 90vw; margin-left: 5vw;  display: flex;justify-content:space-around;">
      <view class="xuan"><view>已选<text style="color: rgba(80, 177, 178, 1);">{{numbs}}</text>个企业</view>
      <view bind:tap="quanxuans"><image src="/pkgA/img/圆形 1.png" mode=""/><image style="display: {{quanxuan?'':'none'}};"  class="imggg" src="/pkgA/img/圆形 2.png" mode=""/><view style="position: absolute;left: 13vw;">全选</view></view>
      </view>
      <view class="deletes" bind:tap="deletes">取消收藏</view>
      <view class="copys" bind:tap="copys">复制投递信息</view>
    </view>
  </view>
</scroll-view>
