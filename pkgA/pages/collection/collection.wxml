<view class="sdsf" catchtouchmove="true" bindtap='hideview' style="display:{{types1?'block':'none'}}; height: {{toppx}}px; width: 100vw; z-index: 200030; position: fixed; top: 0;"></view>
<view class="bg" catchtouchmove="true" bindtap='hideview' style="display:{{types1?'block':'none'}}; top: {{toppx}}px;"></view>
<view catchtouchmove="true" style="top:{{toppx-10}}px; width:100vw; position: absolute; z-index: 2001;">
  <view class="zhezhao" wx:for="{{leixin}}" wx:key="index" style="display: {{types1?'block':'none'}};">
    <view class="zg" id="{{index}}" bind:tap="chooseop" style="color: {{item.ch?'rgba(80, 177, 178, 1);':'rgba(25, 25, 25, 1);' }};"><text>{{item.op}}</text>
      <image src="{{item.ch?'/img/xuan.png':''}}" mode="" />
    </view>
  </view>
</view>
<!-- fjsghjsfhgk -->
<view class="sdsf" catchtouchmove="true" bindtap='hideview' style="display:{{types2?'block':'none'}}; height: {{toppx}}px; width: 100vw; z-index: 200030; position: fixed; top: 0;"></view>
<view class="bg" catchtouchmove="true" bindtap='hideview' style="display:{{types2?'block':'none'}}; top: {{toppx}}px;"></view>
<view catchtouchmove="true" style="top:{{toppx-10}}px; width:100vw; position: absolute; z-index: 2001;">
  <view class="zhezhao" wx:for="{{zhuangtai}}" wx:key="index" style="display: {{types2?'block':'none'}};">
    <view class="zg" id="{{index}}" bind:tap="choosetai" style="color: {{item.ch?'rgba(80, 177, 178, 1);':'rgba(25, 25, 25, 1);' }};"><text>{{item.op}}</text>
      <image src="{{item.ch?'/img/xuan.png':''}}" mode="" />
    </view>
  </view>
</view>